{
    /**
     * The name of the package.
     */
    "name": "lib-cn_comp",

    "sencha": {
        "namespace": "coon.comp",
        "type": "code",
        "toolkit": "classic",
        "creator": "coon.js",
        "summary": "A collection of enhanced views and related functionality for Sencha ExtJS applications.",
        "detailedDescription": "This package contains extended and advanced implementations for ExtJS projects which are related to view components. Requires lib-cn_core. Also home of the Livegrid-plugin.",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": null,
        "output": {
            "base" : "${package.dir}/build",
            "js": {
                "version": "ES6"
            }
        },
        "compressor": {
            "polyfills": "none"
        },
        "local": true,
        "sass" : null,
        "classpath": [
            "${package.dir}/src",
            "${package.dir}/${toolkit.name}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "example": null,
        "framework": "ext",
        "requires": [
            "lib-cn_core"
        ]
    }
}
